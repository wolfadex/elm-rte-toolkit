{
    "type": "package",
    "name": "wolfadex/elm-rte-toolkit",
    "summary": "Build rich text editors in Elm (fork of mweiss/elm-rte-toolkit)",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "exposed-modules": [
        "RichText.Annotation",
        "RichText.Commands",
        "RichText.Config.Command",
        "RichText.Config.Decorations",
        "RichText.Config.ElementDefinition",
        "RichText.Config.Keys",
        "RichText.Config.MarkDefinition",
        "RichText.Config.Spec",
        "RichText.Definitions",
        "RichText.Editor",
        "RichText.Html",
        "RichText.List",
        "RichText.Model.Attribute",
        "RichText.Model.Element",
        "RichText.Model.History",
        "RichText.Model.HtmlNode",
        "RichText.Model.InlineElement",
        "RichText.Model.Mark",
        "RichText.Model.Node",
        "RichText.Model.Selection",
        "RichText.Model.State",
        "RichText.Model.Text",
        "RichText.Node",
        "RichText.State"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0",
        "elm/json": "1.0.0 <= v < 2.0.0",
        "elm/regex": "1.0.0 <= v < 2.0.0",
        "elm-community/array-extra": "2.1.0 <= v < 3.0.0",
        "elm-community/json-extra": "4.0.0 <= v < 5.0.0",
        "elm-community/list-extra": "8.0.0 <= v < 9.0.0",
        "folkertdev/elm-deque": "3.0.1 <= v < 4.0.0",
        "hecrj/html-parser": "2.3.4 <= v < 3.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "2.2.0 <= v < 3.0.0"
    }
}
